	<grid>
		<row>
			<button text="U" />
			<button text="J" ontap="jump" />
		</row>
		<row>
			<button text="L" ontap="attack" />
			<button text="R" />
		</row>
		<row>
			<button text="C" />
			<button text="S" />
		</row>
	</grid>
	<touch ontouchdelta="look" ondown="look_down" onup="look_up" ontap="look_tap" />
	<touch ontouchdelta="foo" />
	<touch ontouchdelta="asdf" />
</layout>