<?xml version="1.0" encoding="utf-8"?>
<layout>
	<row weight="wrap">
		<label id="info" />
	</row>
	<row>
		<touch 
			id="touch"
			ontouchstart="touch_start"
			ontouchend="touch_end"
			ontouchabs="touch_abs"
			ontouchsize="touch_size"
			/>
	</row>
	<row weight="wrap">
		<slider onchange="throttle" text="Throttle" progress="50" progressmax="100" />
	</row>
</layout>